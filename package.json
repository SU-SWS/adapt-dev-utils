{
  "name": "adapt-dev-utils",
  "description": "Provides CLI commands for interacting with ADAPT projects.",
  "version": "1.0.0-dev.0",
  "author": "Nathan Plowman @nplowman",
  "bin": {
    "adapt-utils": "./bin/run"
  },
  "bugs": "https://github.com/SU-SWS/adapt-dev-utils/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/SU-SWS/adapt-dev-utils",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "adapt-utils",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./src/hooks/init/set-env"
      ]
    }
  },
  "repository": "SU-SWS/adapt-dev-utils",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.18",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^5.2.2",
    "cli-ux": "^5.5.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.5.2",
    "netlify": "^6.1.29",
    "storyblok-js-client": "^4.0.9"
  }
}
